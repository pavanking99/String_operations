import java.util.*;
class substring
{
public static void main(String arg[])
{
System.out.print("2.substring or not\n");
Scanner sc=new Scanner(System.in);
String s1,s2;
System.out.print("enter first string:");
s1=sc.nextLine();
System.out.print("enter sub string:");
s2=sc.nextLine();
if(s1.contains(s2))
System.out.print("yes");
else
{
System.out.print("no");
}
}
}
